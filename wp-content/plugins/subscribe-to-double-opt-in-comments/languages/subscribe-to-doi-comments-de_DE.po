msgid "All notifications that were formerly sent to <strong>%1$s</strong> will now be sent to <strong>%2$s</strong>!"
msgstr "Alle Benachrichtigungen die vorher an <strong>%1$s</strong> gingen, gehen jetzt an <strong>%2$s</strong>!"

#: subscribe-to-doi-comments.php:1074
msgid "<strong>%1$s</strong> %2$s removed successfully."
msgstr "<strong>%1$s</strong> %2$s wurde erfolgreich entfernt."

#: subscribe-to-doi-comments.php:1074
msgid "subscriptions"
msgstr "Abonnements"

#: subscribe-to-doi-comments.php:1074
msgid "subscription"
msgstr "Abonnement"

