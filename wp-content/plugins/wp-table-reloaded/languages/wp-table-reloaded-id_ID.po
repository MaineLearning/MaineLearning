msgstr "Sorot sebuah baris dengan mengganti warna latarnya ketika kursor tetikus melayang di atasnya."

# @ wp-table-reloaded
#: views/view-edit.php:275
msgid "Table head"
msgstr "Tajuk tabel"

# @ wp-table-reloaded
#: views/view-edit.php:276
